{
  "name": "ecommerce-api",
  "version": "1.0.0",
  "description": "eCommerce API to put your store online",
  "main": "dist/src/index.js",
  "scripts": {
    "lint": "eslint .",
    "prettier": "prettier --config .prettierrc '**.ts' --write",
    "format": "npm run lint && npm run prettier",
    "build": "npx tsc --build",
    "migrate-test": "db-migrate db:create test_db && db-migrate up -e test",
    "test": "db-migrate db:drop test_db && npm run build && npm run migrate-test && ENV=test jasmine && db-migrate db:drop test_db",
    "start": "npm run build && node ."
  },
  "author": "Omar Sabra",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jasmine": "^4.2.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.3",
    "supertest": "^6.2.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.0.3",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^18.0.1",
    "@types/pg": "^8.6.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-spec-reporter": "^7.0.0",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  }
}
